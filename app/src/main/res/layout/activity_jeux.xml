<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:viewgrille="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_jeux"
    android:background="@drawable/img">

    <view
        android:id="@+id/view_grille"
        class="com.example.smartsudoku.Grille"
        id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="459dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        viewgrille:layout_constraintVertical_bias="0.439" />

    <Button
        android:id="@+id/button10"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_button"
        android:onClick="Result_Game"
        android:text="Valider"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        viewgrille:layout_constraintBottom_toBottomOf="parent"
        viewgrille:layout_constraintEnd_toEndOf="parent"
        viewgrille:layout_constraintHorizontal_bias="0.0"
        viewgrille:layout_constraintStart_toStartOf="parent"
        viewgrille:layout_constraintTop_toBottomOf="@+id/view_grille"
        viewgrille:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="150dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/logo"
        viewgrille:layout_constraintBottom_toTopOf="@+id/view_grille"
        viewgrille:layout_constraintEnd_toEndOf="parent"
        viewgrille:layout_constraintHorizontal_bias="0.498"
        viewgrille:layout_constraintStart_toStartOf="parent"
        viewgrille:layout_constraintTop_toTopOf="parent"
        viewgrille:layout_constraintVertical_bias="0.253" />


</androidx.constraintlayout.widget.ConstraintLayout>
